= simple_form_for @dealer, :html => {:class => 'form-horizontal', :role => 'form'} do |f|

  = f.input :name
  = f.input :contact_name
  = f.input :phone_number
  = f.input :email


  .btn-toolbar.text-center
    = f.button :submit, t('views.common.save')
    %a.btn.btn-default{:href => dealers_path}= t('views.common.cancel')
